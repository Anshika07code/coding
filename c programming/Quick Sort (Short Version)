#include <stdio.h>
void swap(int *a, int *b) {
    int t = *a; *a = *b; *b = t;
}
int part(int a[], int l, int h) {
    int p = a[h], i = l - 1;
    for (int j = l; j < h; j++)
        if (a[j] < p) swap(&a[++i], &a[j]);
    swap(&a[i + 1], &a[h]);
    return i + 1;
}
void qs(int a[], int l, int h) {
    if (l < h) {
        int p = part(a, l, h);
        qs(a, l, p - 1);
        qs(a, p + 1, h);
    }
}
